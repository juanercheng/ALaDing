@import '../common/my-common'
@import '../common/header'
@import '../common/footer'

.record
  width 600px
  margin auto
  margin-top 30px

.record li
  padding 5px 0
  &:first-child,
  &:last-child
    border-bottom 1px solid #d3d3d3
  &:last-child
    padding-bottom 20px
  &:first-child
    margin-bottom 10px
  & span
    display inline-block
    width 22%
    &:first-child
      width 10%
