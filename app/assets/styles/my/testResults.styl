@import '../common/my-common'
@import '../common/header'
@import '../common/footer'
@import '../reg-step2'
@import '../common/reg-common'


.container .my_right .content{
  background-color #fff
}
.resultBox
  text-align center
  margin-top 25px

.btnBox
  text-align center
  margin-top 60px

.btnBox .btn{
  margin-right 10px
  width 23%
  height 45px
  display inline-block !important
}
.result h2{
  margin: 45px 0;
  color: #666;
  font-weight: normal;
  text-align center
}
.resultFin p,.result p{
  color #666
  text-align center
}
form .file .file_upload {
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  border-radius: 10px;
  position: absolute;
  width: 360px;
  height: 100px;
  display: inline-block;
  top: 0;
  left: 33%;
  overflow: hidden;
  background: #dcdcdc;
}
form .input>label {
  width: 341px;
  text-align: right;
  color: #000;
}
form .file .file_upload{
  overflow: hidden;
}
form .file input[type="file"] {
  position absolute
  left 0px
  top 0px
}
.file_upload img{
  width 100%
  height 100%
}